<p id="notice"><%= notice %></p>

<p>
  <b>Order time:</b>
  <%= @order.order_time %>
</p>

<p>
  <b>Price:</b>
  <%= @order.price %>
</p>

<p>
  <b>Delivered:</b>
  <%= @order.delivered %>
</p>

<%="Order inspect:"%>
<%=@order.inspect %>
<% for preference in @order.preferences %>
	<p>
	<b>Quantity:</b>
	<%= preference.quantity %>
	</p>

	<p>
	<b>Size:</b>
	<%= preference.size %>
	</p>

	<p>
	<b>Crust:</b>
	<%= preference.crust %>
	</p>

	<p>
	<b>Price:</b>
	<%= preference.price %>
	</p>
	<%="Preference inspect:"%>
	<%=preference.inspect%>
	<% for ingredient in preference.ingredients %>
	<p>
	<b>Ingredient Name:</b>
	<%= ingredient.ingred_name %>
	</p>
	<% end %>

<% end %>


<%= link_to 'Edit', edit_order_path(@order) %> |
<%= link_to 'Back', orders_path %>