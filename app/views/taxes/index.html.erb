<h1>Listing taxes</h1>

<table>
  <tr>
    <th>State</th>
    <th>Tax name</th>
    <th>Amount</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @taxes.each do |tax| %>
  <tr>
    <td><%= tax.state %></td>
    <td><%= tax.tax_name %></td>
    <td><%= tax.amount %></td>
    <td><%= link_to 'Show', tax %></td>
    <td><%= link_to 'Edit', edit_tax_path(tax) %></td>
    <td><%= link_to 'Destroy', tax, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Tax', new_tax_path %>
